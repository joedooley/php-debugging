{
    "name" 			: "devdesigns/php-debugging",
    "description" 	: "Making PHP debugging easier for WordPress development",
    "type"          : "project",
    "prefer-stable" : true,
    "keywords" 		: ["wordpress", "wp", "plugin"],
    "homepage" 		: "https://www.developingdesigns.com/",
    "license" 		: "GPL-2.0+",
    "authors": [
        {
            "name" 	: "joedooley",
            "email" : "hello@developingdesigns.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "kint-php/kint" : "~2.0",
        "filp/whoops": "~2.0",
        "mockery/mockery": "0.9.*",
        "symfony/var-dumper": "^3.2",
        "whoops/soap": "^1.0"
    },
    "autoload" : {
        "classmap": [],
        "files": [
            "src/support/exceptions.php",
            "src/sandbox.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}
